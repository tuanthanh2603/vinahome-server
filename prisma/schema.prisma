model User {
  id Int @id @default(autoincrement())
  username String @unique
  password String
  fullName String?
  address String?
  numberPhone String
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt

  @@map("tbl_users")
}
model Company {
  id Int @id @default(autoincrement())
  name String
  phone String
  address String?
  note String?
  status Boolean
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt

  @@map("tbl_company")
}


